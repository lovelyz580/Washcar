.coupon-list {
  margin: 20rpx;
}

.coupon-list .item {
  width: 100%;
  height: 250rpx;
  margin-bottom: 20rpx;
  border-radius: 20rpx;
}

.coupon-list .item .float-li {
  width: 80%;
  height: 100%;
  border-right: 2rpx dashed rgba(255, 255, 255, 0.3);
  float: left;
}

.coupon-list .item .float-li-rig {
  width: 10%;
  padding-right: 20rpx;
  height: 100%;
  color: #fff;
  float: left;
}

.coupon-left {
  position: relative;
}

.coupon-left .t {
  position: absolute;
  color: #fff;
}

.coupon-left .t1 {
  left: 50rpx;
  top: 110rpx;
}

.coupon-left .t2 {
  left: 80rpx;
  top: 50rpx;
  font-size: 80rpx;
  font-weight: bold;
}

.coupon-left .t3 {
  left: 250rpx;
  top: 100rpx;
  width: 200rpx;
  font-size: 30rpx;
}

.coupon-left .t4 {
  left: 50rpx;
  top: 180rpx;
}
