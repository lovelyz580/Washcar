/* pages/orderdetail/orderdetail.wxss */
page {
  background-color: #ededed;
}

/* 导航 */

.choose {
  color: slateblue;
}

swiper {
  width: 100%;
  height: 250rpx;
}

swiper image {
  width: 100%;
  height: 250rpx;
}

.tt {
  width: 100%;
  height: 100rpx;
  text-align: left;
  font-size: 30rpx;
  color: #666;
  margin-left: 20rpx;
  display: block;
}

.tt text {
  line-height: 120rpx;
}

.wrap {
  /* width: 100%; */
  margin: 20rpx;
}

.checkbox-con {
  margin-top: 40rpx;
  text-align: center;
  background: #f8f9fb;
  border-radius: 20rpx;
}

.checkbox {
  /* width: 45%; */
  height: 72rpx;
  line-height: 72rpx;
  font-size: 28rpx;
  color: #888;
  border: 1rpx solid #cecece;
  border-radius: 20rpx;
  display: inline-block;
  margin: 10rpx 20rpx 10rpx 0;
  padding: 0 30rpx;
  position: relative;
  float: left;
}

.checked {
  color: #1a92ec;
  background: rgba(49, 165, 253, 0.08);
  border: 1rpx solid #31a5fd;
}

.checkbox checkbox {
  display: none;
}

.checked-img {
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  top: 0;
  right: 0;
}

.radio-cl {
  height: 100rpx;
  margin-left: 20rpx;
  display: block;
}

.radio-qj {
  height: 230rpx;
  margin-left: 20rpx;
  display: block;
}

.thickLine {
  height: 20rpx;
  background: #ededed;
}

#adduser {
  background: #f5f5f5;
}

.section__title {
  float: left;
  width: 25%;
  text-align: left;
}

.section__title_heji {
  float: left;
  width: 50%;
  text-align: right;
  margin-right: 10%;
}

.form-group {
  float: right;
  width: 75%;
  text-align: left;
}

.form-group_heji {
  float: right;
  width: 40%;
}

.s_red {
  color: red !important;
  font-size: 50rpx !important;
  margin: 0 10rpx;
}

.section {
  clear: both;
  margin: 0 20rpx;
  height: 120rpx;
  line-height: 120rpx;
  font-size: 30rpx;
  color: #666;
  border-bottom: 1px #ededed solid;
}

.input-text {
  border: none;
  height: 120rpx;
  text-align: left;
}

.button {
  border: 1px solid #3890f3;
  background: #3890f3;
  border-radius: 2px;
}

.picker {
  height: 120rpx;
  text-align: left;
  background: url("data:image/png;base64,") right no-repeat;
  background-size: 30rpx 30rpx;
}

.btn {
  background: #3890f3 !important;
  color: #fff;
  border-radius: 20rpx;
}

.mt40 {
  margin-top: 40rpx;
}



.item {
  position: relative;
  margin: 10rpx 5rpx;
  width: 200rpx;
  height: 200rpx;
}




.tipTitle {
  text-align: center;
  font-size: 30rpx;
  color: #f6a29d;
  font-weight: 700;
  width: 100%;
  margin: 50rpx 0;
}

.boxStyle {
  width: 300rpx;
  height: 100rpx;
  position: absolute;
  bottom: -1rpx;
  border-radius: 50%;
  /* box-shadow:0rpx 10rpx 100rpx #fddbd9; */
  margin-left: 35rpx;
}

.height200 {
  /* height: 240rpx; */
  height: 100%;
  min-height: 240rpx;
  line-height: 240rpx;
}

/* 上传图片 */
.add-image, .up-pic image {
  margin-top: 2%;
  margin-left: 2%;
  width: 150rpx;
  height: 150rpx;
  color: #ddd;
  font-size: 144rpx;
  line-height: 95%;
  text-align: center;
  background-color: #fff;
  cursor: pointer;
  border-radius: 10rpx;
}

.picture_list {
  padding: 20rpx;
  margin-left: 12%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: flex-start;
  align-content: flex-start;
}

.picture_item {
  margin: 10px;
  margin-left: 0;
  position: relative;
  width: 160rpx;
  height: 160rpx;
}

.picture_item .del {
  position: absolute;
  top: -11rpx;
  right: -11rpx;
  color: #fff;
  border-radius: 50%;
  width: 40rpx;
  height: 40rpx;
  z-index: 2;
  line-height: 35rpx;
  text-align: center;
  background-color: #f00;
}

.upload_progress {
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0.7;
  border-radius: 8rpx;
  background-color: #000;
  color: #fff;
  width: 160rpx;
  height: 160rpx;
  text-align: center;
  line-height: 160rpx;
  font-size: 12px;
}

.picture_item image {
  width: 160rpx;
  height: 160rpx;
}
/*星星*/
.comment1-description{
  padding:30rpx 0;
  display:flex;
  flex-direction:row;
}
.stars{
    background:url("data:image/png;base64,");
    width: 80rpx;
